   aria-labelledby = "myModalLabel">
      <div class = "modal-dialog">
      <div class = "modal-content" style="rgb(0, 0, 0) radial-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.8), rgb(0, 0, 0)) repeat scroll 0% 0% !important">

         <div class = "modal-header">
  <i class="fa fa-times closeme pull-right"></i>          
    <h3 class = "modal-title" id = "myModalLabel">
               <b>if you use a life, you can skip this level.</b>
            </h3>
	<br/><button class="btn btn-default accept" style="background:rgb(220,0,0);outline:none;border:none;border-radius:0;color:#fff">I Accept</button>
