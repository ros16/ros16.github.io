<script>
$(document).ready(function(){
//init home box
setTimeout(function(){$("#home-box").addClass("popOut");},500);//popOut later

anims = ['popOut','popIn'];

animQuestion();

function animQuestion()
{
//alert("aq");
$("#home-box").fadeIn(100).addClass("popOut");//popOut later
$("#home-box").find(".ros-pic").each(function(i){
var elem = $(this);
setTimeout(function(){$(elem).addClass("popOut");},i*400+500);
});
}
$(".navelem").click(function(){
  
  $(".box").animate({'margin-left':"-1500px"},0,'easeOutSine')
  $("#home-box").fadeOut(0).removeClass("popOut");
  //$(".ros-pic").removeClass("popOut");
  var idd = "#"+$(this).attr("id")+"-box";
  if(idd == '#home-box')
	animQuestion();
  else
	$(idd).animate({'margin-left':"25%"},10,'easeInSine');
});

//modal
$(".fa-heart").click(function(){
	if(!$(this).hasClass("lived"))
	{
		$("#myModal1").show();
		
		$(".container").css({'filter':"blur(30px)",'-webkit-filter':"blur(30px)"});
		heart = $(this);
		//skip level
		$(".accept").click(function()
		{
		$(heart).addClass("lived");
		$(".container").css({'filter':"blur(0px)",'-webkit-filter':"blur(0px)"});
		$("#myModal1").hide();
		});

	}
});


//close modal
$(".closeme").click(function(){
$('#myModal1').hide(0);
$(".container").css({'filter':"blur(0px)",'-webkit-filter':"blur(0px)"});
});
});

</script>
